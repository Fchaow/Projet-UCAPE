{% extends 'base.html.twig' %}

{% block body %}
        <a href="{{path('listeExaminateur')}}">Afficher tous les examinateurs</a>
        <h1>Modification de la fiche de l'examinateur</h1>
        {{ form_start(formModif) }}
        <div style="width:200px">
            {{ form_row(formModif.nomExaminateur) }}
        </div>
        <div style="width:200px">
            {{ form_row(formModif.prenomExaminateur) }}
        </div>
        <div style="width:200px">
            {{ form_row(formModif.telephoneExaminateur) }}
        </div>
        <div style="width:200px">
            {{ form_row(formModif.langues) }}
        </div>
        {{ form_row(formModif.submit) }}
        
        {{ form_end(formModif) }}

{% endblock %}