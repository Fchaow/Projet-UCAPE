<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\Examinateur" table="examinateur">
    <indexes>
      <index name="Eid_langue_examinateur" columns="Eid_langue_examinateur"/>
    </indexes>
    <id name="idExaminateur" type="integer" column="id_examinateur">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="nomExaminateur" type="string" column="nom_examinateur" length="50" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="prenomExaminateur" type="string" column="prenom_examinateur" length="50" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="telephoneExaminateur" type="string" column="telephone_examinateur" length="50" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <many-to-one field="eidLangueExaminateur" target-entity="Langue" fetch="LAZY">
      <join-columns>
        <join-column name="Eid_langue_examinateur" referenced-column-name="id_langue"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
