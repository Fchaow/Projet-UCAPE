<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\Classe" table="classe">
    <id name="idClasse" type="integer" column="id_classe">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="libelleClasse" type="string" column="libelle_classe" length="50" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <many-to-many field="eidEleveAppartenir" target-entity="Eleve" inversed-by="eidClasse" fetch="LAZY">
      <join-table name="appartenir">
        <join-columns>
          <join-column name="Eid_classe" referenced-column-name="id_classe"/>
        </join-columns>
        <inverse-join-columns>
          <join-column name="Eid_eleve_appartenir" referenced-column-name="id_eleve"/>
        </inverse-join-columns>
      </join-table>
    </many-to-many>
  </entity>
</doctrine-mapping>
