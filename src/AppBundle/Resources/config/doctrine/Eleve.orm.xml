<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\Eleve" table="eleve">
    <id name="idEleve" type="integer" column="id_eleve">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="nomEleve" type="string" column="nom_eleve" length="50" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="prenomEleve" type="string" column="prenom_eleve" length="50" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="sexeEleve" type="string" column="sexe_eleve" length="50" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="dateNaissEleve" type="date" column="date_naiss_eleve" nullable="false"/>
    <field name="promoEleve" type="integer" column="promo_eleve" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="emailEleve" type="string" column="email_eleve" length="50" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="emailParentEleve" type="string" column="email_parent_eleve" length="50" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="motDePasseEleve" type="string" column="mot_de_passe_eleve" length="50" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="commentairesGeneralEleve" type="text" column="commentaires_general_eleve" length="65535" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="terreDesLanguesEleve" type="boolean" column="terre_des_langues_eleve" nullable="false"/>
    <field name="commentairesChoixEleve" type="text" column="commentaires_choix_eleve" length="65535" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="visaParentEleve" type="boolean" column="visa_parent_eleve" nullable="false"/>
    <field name="ue2DateEleve" type="date" column="UE2_date_eleve" nullable="false"/>
    <field name="ue2ThemeDossierEleve" type="string" column="UE2_theme_dossier_eleve" length="50" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="ue2NoteEleve" type="float" column="UE2_note_eleve" precision="10" scale="0" nullable="false"/>
    <field name="ue2AppreciationsEleve" type="text" column="UE2_appreciations_eleve" length="65535" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="typeEleve" type="string" column="type_eleve" length="1" nullable="false">
      <options>
        <option name="fixed">1</option>
      </options>
    </field>
    <field name="ue1DateUcape" type="date" column="UE1_date_ucape" nullable="true"/>
    <field name="ue1NoteUcape" type="float" column="UE1_note_ucape" precision="10" scale="0" nullable="true"/>
    <field name="ue1AppreciationsUcape" type="text" column="UE1_appreciations_ucape" length="65535" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="obtentionDiplomeUcape" type="boolean" column="obtention_diplome_ucape" nullable="true"/>
    <field name="mentionUcape" type="string" column="mention_ucape" length="50" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="commentairesUcape" type="text" column="commentaires_ucape" length="65535" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <many-to-many field="eidClasse" target-entity="Classe" mapped-by="eidEleveAppartenir" fetch="LAZY"/>
    <many-to-many field="eidPaysChoix" target-entity="Pays" mapped-by="eidEleveChoix" fetch="LAZY"/>
    <many-to-many field="eidEtablissement" target-entity="Etablisement" inversed-by="eidEleveProposition" fetch="LAZY">
      <join-table name="proposition">
        <join-columns>
          <join-column name="Eid_eleve_proposition" referenced-column-name="id_eleve"/>
        </join-columns>
        <inverse-join-columns>
          <join-column name="Eid_etablissement" referenced-column-name="id_etablissement"/>
        </inverse-join-columns>
      </join-table>
    </many-to-many>
  </entity>
</doctrine-mapping>
