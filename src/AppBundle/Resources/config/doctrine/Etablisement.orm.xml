<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\Etablisement" table="etablisement">
    <indexes>
      <index name="Eid_pays" columns="Eid_pays"/>
    </indexes>
    <id name="idEtablissement" type="integer" column="id_etablissement">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="libelleEtablissement" type="string" column="libelle_etablissement" length="50" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="nomEtablissement" type="string" column="nom_etablissement" length="50" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="telEtablissement" type="string" column="tel_etablissement" length="50" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="emailEtablissement" type="string" column="email_etablissement" length="50" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="responsableEtablissement" type="string" column="responsable_etablissement" length="50" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="numeroEtablissement" type="string" column="numero_etablissement" length="50" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="rueEtablissement" type="string" column="rue_etablissement" length="50" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="villeEtablissement" type="string" column="ville_etablissement" length="50" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <many-to-one field="eidPays" target-entity="Pays" fetch="LAZY">
      <join-columns>
        <join-column name="Eid_pays" referenced-column-name="id_pays"/>
      </join-columns>
    </many-to-one>
    <many-to-many field="eidEleveProposition" target-entity="Eleve" mapped-by="eidEtablissement" fetch="LAZY"/>
  </entity>
</doctrine-mapping>
