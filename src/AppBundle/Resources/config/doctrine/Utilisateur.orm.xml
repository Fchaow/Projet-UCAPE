<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\Utilisateur" table="utilisateur">
    <id name="codeUtilisateur" type="integer" column="code_utilisateur">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="identifiantUtilisateur" type="string" column="identifiant_utilisateur" length="50" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="motDePasseUtilisateur" type="string" column="mot_de_passe_utilisateur" length="50" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
