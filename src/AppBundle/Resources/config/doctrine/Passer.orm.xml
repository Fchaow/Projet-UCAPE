<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\Passer" table="passer">
    <indexes>
      <index name="Eid_langue_passer" columns="Eid_langue_passer"/>
      <index name="Eid_examinateur" columns="Eid_examinateur"/>
      <index name="Eid_eleve_passer" columns="Eid_eleve_passer"/>
    </indexes>
    <id name="eidElevePasser" association-key="true"/>
    <id name="eidExaminateur" association-key="true"/>
    <id name="eidLanguePasser" association-key="true"/>
    <field name="datePasser" type="date" column="date_passer" nullable="false"/>
    <field name="notePasser" type="float" column="note_passer" precision="10" scale="0" nullable="false"/>
    <field name="appreciationPasser" type="text" column="appreciation_passer" length="65535" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <one-to-one field="eidElevePasser" target-entity="Eleve" fetch="LAZY">
      <join-columns>
        <join-column name="Eid_eleve_passer" referenced-column-name="id_eleve"/>
      </join-columns>
    </one-to-one>
    <one-to-one field="eidExaminateur" target-entity="Examinateur" fetch="LAZY">
      <join-columns>
        <join-column name="Eid_examinateur" referenced-column-name="id_examinateur"/>
      </join-columns>
    </one-to-one>
    <one-to-one field="eidLanguePasser" target-entity="Langue" fetch="LAZY">
      <join-columns>
        <join-column name="Eid_langue_passer" referenced-column-name="id_langue"/>
      </join-columns>
    </one-to-one>
  </entity>
</doctrine-mapping>
